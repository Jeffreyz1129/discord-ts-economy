{
  "name": "discord-ts-economy",
  "version": "0.1.5",
  "description": "A simple discord bot economy system using mongodb and typescript.",
  "main": "./out/index.js",
  "files": [
    "out"
  ],
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "scripts": {
    "test": "node ./__tests__/__db__.js",
    "dev": "ts-node ./lib/index.ts",
    "prod:dev": "yarn run build && node .",
    "make": "yarn run format && yarn run build && yarn run docs",
    "build": "tsc --skipLibCheck",
    "format": "prettier --ignore-path .gitignore --write .",
    "update": "yarn upgrade-interactive --latest",
    "watch": "tsc -w",
    "docs": "npx typedoc --out docs --entryPointStrategy expand ./lib --json docs/v1.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lewd-labs/Economy.git"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "mongodb",
    "discord-economy",
    "economy"
  ],
  "private": false,
  "engines": {
    "node": ">=6.0.0"
  },
  "engineStrict": true,
  "author": "ThatGuyJamal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lewd-labs/Economy/issues"
  },
  "homepage": "https://lewd-labs.github.io/xyz/",
  "devDependencies": {
    "@types/node": "^17.0.0",
    "dotenv": "^10.0.0",
    "prettier": "^2.4.1",
    "typedoc": "^0.22.10",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@discordjs/collection": "0.3.2",
    "mongoose": "^6.1.2",
    "tslib": "^2.3.1"
  }
}
